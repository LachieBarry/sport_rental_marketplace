<% @bookings.each do |booking| %>
    <div class="col-xl-4 col-lg-6 col-xs-12">
        <div class="card rounded border-primary" style="width: 18rem;">
        <%= cl_image_tag booking.listing.sport_type.artwork.key, height: 300, width: 300, crop: :fill %>
        <div class="card-body">
            <h5 class="card-title"><%= booking.listing.title %></h5>
            <div class="card-text">
            <strong>From:</strong> <%= booking.start_date %> <br>
            <strong>To:</strong> <%= booking.end_date %> <br>
            <strong>Price:</strong> $ <%= booking.hourly_price.to_i.to_s %> <br>
            <strong>Sport Type:</strong> <%= booking.listing.sport_type %> <br>
            <strong>Rented from:</strong> <%= booking.listing.user.first_name.capitalize %>
            </div>
        </div>
        </div>
    </div>
<% end %>